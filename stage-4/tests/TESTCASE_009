vector[10] int;

//Testing simple commands:

int print_xis(int xx)
{

}

int simple_commands () {
  // Variable declaration:
  int xis;
  static float x;
  static const float y <= 3.1415;
  char y2 <= '\n';
  string hhh <= "string";

  // Attribuition commands:
  xis = 2;
  vector[2] = xis * xis;

  // I/O commands:
  output("Digite um valor para xis");
  input xis;

  // Function call:
  print_xis(xis);
  print_xis(2*3*xis);

  // Shift commands:
  xis << 2;
  xis << xis;
  xis >> 10;
  xis >> (xis + (x ^ 2));

  // Return, break, continue:
  return xis;
  break;
  continue;
  int z;
  float k;
  // if, for, while:
  if (x==1) then
  {
    x=2;
    y=3;
    if(z==4) then
    {
      k=k+1;
    }
    else
    {
      k=k+2;
    };
  }
  else
  {
    k=0;
  };

  int i;
  int j;
  for(i=0:i<100:i=i+1)
  {
    j=j+1;
  };

  while(true) do
  {
    output("AAAAAAAAAAAAAAAA\n");
  };

  // Testing expressions:
  test = +x;
  test = -x;
  test = -(-(x^3));
  test = !true;
  test = &xis;
  test = *xis;
  test = &(*(x+1));
  test = ?(!false);
  test = vector[1];
  test = xis + xis + xis;
  test = xis * xis / xis;
  test = xis % 4;
  test = x | y;
  test = y & x;
  test = (x ^ 4) ^ 5;
  test = xis ? (1+9) * 10 : 0;
}

