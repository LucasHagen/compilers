int inc (int x){
    return x + 1;
}


c char;
palavra string;
inteiro int;
flutuante float;
booleano bool;

int main(int x){
    {
        int x;
        x = 1;
    };
    x = x + 2;
    inc(x);
    c = 'c';
    palavra = "palavra";
    inteiro = 10;
    flutuante = 10.7584;
    booleano = true;
}
