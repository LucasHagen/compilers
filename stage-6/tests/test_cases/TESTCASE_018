one int;    // 1
two int;    // 2
three int;  // 3
four int;   // 5
five int;   // 8

int fat(int i){
    if(i > 1) then
    {
        return i * fat(i - 1);
    }
    else
    {
        return 1;
    };
 }

int main(){
    one   = fat(1);
    two   = fat(2);
    three = fat(3);
    four  = fat(4);
    five  = fat(5);
 }
